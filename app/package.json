{
  "name": "@amazon-connect/app",
  "version": "1.0.2",
  "description": "App functionality of the Amazon Connect SDK",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf ./lib && rm -rf ./build",
    "prepack": "npm run build",
    "test": "jest --coverage"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/amazon-connect/AmazonConnectSDK.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@amazon-connect/workspace-types": "1.0.2",
    "@amazon-connect/core": "1.0.2"
  }
}
