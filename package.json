{
  "name": "@amazon-connect/sdk",
  "version": "1.0.0",
  "description": "",
  "npm-pretty-much": {
    "useWorkspaces": true,
    "publishLibCommonJs": true
  },
  "scripts": {
    "agent": "npm run --prefix ./agent",
    "app": "npm run --prefix ./app",
    "build": "npm run --ws build",
    "check-types": "npm run --ws check-types",
    "clean": "npm run --ws clean",
    "core": "npm run --prefix ./core",
    "format": "eslint . --fix",
